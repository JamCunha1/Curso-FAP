const readline = require('readline-sync')

function menu () {
    let opcao = parseInt(readline.question('********************\n        MENU         \n******************** \n 1 - Listar Clientes  \n 2 - Cadastrar Clientes \n 3 - Editar Clientes \n 4 - Remover Clientes \n 5 - Sair do sistema \n********************\n Opcao: '));
}
menu();